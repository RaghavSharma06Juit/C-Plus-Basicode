    #include<iostream>
    using namespace std;
    int main(){

    int n;
    cout<<"No is ";
    cin>>n;
    bool flag = true;
    for(int i=2;i<=n-1;i++){
        if(n%i==0){
           flag = false;
            break;
        }
        }
      if(n==1) cout<<" no composite no prime";
      else if(flag==true) cout<<n<<" prime";
      else cout<<n<<" is composite";
    

    }
